  "main": "index.js",
  "scripts": {
    "get-schema": "graphql get-schema -p prisma --dotenv config/dev.env",
    "deploy": "prisma deploy --env-file config/dev.env",
    "deploy:prod": "prisma deploy --env-file config/prod.env",
    "build:dev": "webpack",
    "build:prod": "webpack -p --env production",
    "build:babel": "babel src --out-dir dist --copy-files",
    "start": "node dist/index.js",
    "start:dev": "env-cmd ./config/dev.env node dist/index.js",
    "start:dev2": "env-cmd ./config/dev.env nodemon src/index.js --ext js,graphql --exec babel-node"
